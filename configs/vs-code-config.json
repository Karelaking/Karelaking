  "breadcrumbs.enabled": false,
  "window.menuBarVisibility": "compact",
  "editor.mouseWheelZoom": true,
  "files.autoSave": "afterDelay",
  "workbench.iconTheme": "dark-minimalist-icons",
  "workbench.colorTheme": "Default Dark+",
  "tabnine.experimentalAutoImports": true,
  "editor.renderWhitespace": "boundary",
  "editor.wordWrap": "on",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.codeActions.triggerOnFocusChange": true,
  "editor.inlineSuggest.suppressSuggestions": true,
  "editor.suggest.preview": true,
  "editor.guides.indentation": true,
  "editor.suggestSelection": "recentlyUsed",
  "files.autoSaveWhenNoErrors": true,
  "files.autoSaveWorkspaceFilesOnly": true,
  "files.insertFinalNewline": true,
  "RainbowBrackets.depreciation-notice": true,
  "editor.fontSize": 15,
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  // Vim Mode Settings
  "vim.camelCaseMotion.enable": true,
  "vim.easymotion": true,
  "vim.enableNeovim": true,
  "vim.smartRelativeLine": true,
  "vim.highlightedyank.enable": true,
  "vim.hlsearch": true,
  "vim.autoindent": true,
  "vim.autoSwitchInputMethod.enable": true,
  "vim.statusBarColorControl": true,
  "css.format.spaceAroundSelectorSeparator": true,
  "less.format.spaceAroundSelectorSeparator": true,
  "scss.format.spaceAroundSelectorSeparator": true,
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "emmet.useInlineCompletions": true,
  "reactSnippets.settings.prettierEnabled": true,
  "javascript.experimental.updateImportsOnPaste": true,
  "javascript.format.insertSpaceAfterConstructor": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  // type scripts settings
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.autoIndent": "full",
    "editor.bracketPairColorization.enabled": true,
    "editor.cursorWidth": 0,
    "editor.detectIndentation": true,
    "editor.dropIntoEditor.enabled": true,
    "editor.find.loop": true,
    "editor.foldingImportsByDefault": true,
    "editor.suggest.showDelay": 0,
    "editor.hover.above": true,
    "editor.suggest.snippetsPreventQuickSuggestions": true
  },
  "editor.cursorBlinking": "smooth",
  "terminal.integrated.cursorBlinking": true,
  "workbench.sideBar.location": "right",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.minimap.enabled": false,
  "editor.autoIndent": "advanced",
  "editor.fontFamily": "JetBrains Mono, 'Courier New', monospace",
  "terminal.integrated.fontFamily": "JetBrains Mono, 'Courier New",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.bracketPairColorization.enabled": true,
  "editor.multiCursorPaste": "full",
  "editor.cursorWidth": 2,
  "editor.suggest.showVariables": true,
  "editor.suggest.showColors": true,
  "window.dialogStyle": "custom",
  "editor.formatOnPaste": true,
  "editor.fontLigatures": true,
  "javascript.autoClosingTags": true,
  "javascript.format.semicolons": "insert",
  "typescript.autoClosingTags": true,
  "typescript.suggest.autoImports": true,
  "workbench.editor.showIcons": true,
  // "workbench.tree.indent": 8,
  "workbench.editor.mouseBackForwardToNavigate": true,
  "prettier.enable": true,
  "workbench.settings.editor": "ui",
  "workbench.view.alwaysShowHeaderActions": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.pinnedTabsOnSeparateRow": true,
  "workbench.editor.scrollToSwitchTabs": true,
  "editor.autoClosingBrackets": "always",
  "editor.autoClosingQuotes": "always",
  "workbench.editor.openPositioning": "left",
  "editor.tabCompletion": "on",
  "files.autoSaveDelay": 1,
  "files.autoGuessEncoding": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.statusBar.visible": true,
  "diffEditor.wordWrap": "on",
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.smoothScrolling": true,
  "workbench.list.smoothScrolling": true,
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.stickyScroll.enabled": true,
  "workbench.editor.closeOnFileDelete": true,
  "pieces.autocomplete": true,
  "pieces.autocompleteKey": "Enter or Tab",
  "pieces.display": "Preview",
  "editor.scrollbar.horizontalScrollbarSize": 0,
  "editor.scrollbar.verticalScrollbarSize": 0,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "pieces.cloudCapabilities": "Blended",
  "pieces.telemetry": true
}
